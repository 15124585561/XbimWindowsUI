<UserControl x:Class="XbimXplorer.IfcValidation.ValidationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
        >
    <Grid Margin="0" KeyDown="txtCommand_KeyDown">
        <TextBox Margin="0,0,0,22" Name="Results" FontFamily="Lucida Console" AcceptsReturn="True" />
        <TextBox Margin="0,22,0,0" Name="Commands" FontFamily="Lucida Console" AcceptsReturn="True" />
    </Grid>
</UserControl>
